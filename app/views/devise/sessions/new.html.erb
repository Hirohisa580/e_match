  <div class="top-page">  
    <%=render "shared/header/"%>
  </div>
    <div class="session-main">
      <div class="session-border">
        <%= form_with model: @user, url: user_session_path, local: true do |f| %>
          <div class="field">
            <label >メールアドレス<br /><label >
            <%= f.email_field :email, autofocus: true, class: "form-control"%>
          </div>

          <div class="field">
            <label >パスワード<br /><label >
            <%= f.password_field :password, class: "form-control", placeholder: "6文字以上"  %>
          </div>

          <div class="actions">
            <%= f.submit "ログイン", class: "btn btn-success btn-lg mb-3"%>
          </div>
        <% end %>
      </div>
    </div>
